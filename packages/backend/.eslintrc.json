{
    "env": {
        "node": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "comma-dangle": ["error", "never"],
      "no-unused-vars": 1,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0
    }
}
